 template_args = {
            "progress_bar": utils.get_progress_bar(data.get("percentage")),

def get_progress_bar(percentage: float) -> str:
    assert percentage >= 0 and percentage <= 100
    pbar = {
        05: "▰▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱",
        10: "▰▰▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱",
        15: "▰▰▰▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱",
        20: "▰▰▰▰▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱",
        25: "▰▰▰▰▰▱▱▱▱▱▱▱▱▱▱▱▱▱▱",
        30: "▰▰▰▰▰▰▱▱▱▱▱▱▱▱▱▱▱▱▱",
        35: "▰▰▰▰▰▰▰▱▱▱▱▱▱▱▱▱▱▱▱",
        40: "▰▰▰▰▰▰▰▰▱▱▱▱▱▱▱▱▱▱▱",
        45: "▰▰▰▰▰▰▰▰▰▱▱▱▱▱▱▱▱▱▱",
        50: "▰▰▰▰▰▰▰▰▰▰▱▱▱▱▱▱▱▱▱",
        55: "▰▰▰▰▰▰▰▰▰▰▰▱▱▱▱▱▱▱▱",
        60: "▰▰▰▰▰▰▰▰▰▰▰▰▱▱▱▱▱▱▱",
        70: "▰▰▰▰▰▰▰▰▰▰▰▰▰▱▱▱▱▱▱",
        75: "▰▰▰▰▰▰▰▰▰▰▰▰▰▰▱▱▱▱▱",
        80: "▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▱▱▱▱",
        85: "▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▱▱▱",
        90: "▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▱▱",
        95: "▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▱",
        100:"▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰▰",       
    }
    tens, _ = divmod(int(percentage), 5)
    return pbar.get(tens * 19, "▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱▱")
